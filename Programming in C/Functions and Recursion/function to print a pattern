#include <stdio.h>
/*program using function to print the following pattern
*
**
*****/
int main(){
    int n=3;
    for (int i = 0; i < n; i++)
    {
        // Runs from 0-2
        for (int j = 0; j <2*i+1; j++)
        {
            printf("*");
        }
        printf("\n");

    }
    
    return 0;
}

#include <stdio.h>

int main(){
    char ch = 'D';
    printf("You have entered character %c \n",ch);
    printf("Value of the character in ASCII code is %d\n", ch);

    if(ch>= 97 && ch<= 122){
        printf("\nCharacter is lower case");
    }else{
        printf("\nCharacter is not lower case");
    }
    return 0;
}

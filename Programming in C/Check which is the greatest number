#include <stdio.h>

int main()
{
    int a = 120, b = 60, c = 80, d = 100;
    if (a > b && a > c && a > d)
    {
        printf("The greatest number is %d", a);
    }
    else if (b > a && b > c && b > d)
    {
        printf("The greatest number is %d", b);
    }
    else if(c > a && c > b && c > d)
    {
        printf("The greatest number is %d", c);
    }
    else if (d > a && d > b && d > c)
    {
        printf("The greatest number is %d", d);
    }
    return 0;
}
